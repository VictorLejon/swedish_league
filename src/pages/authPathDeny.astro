---
import Layout from '../layouts/Layout.astro';
import { authentication }  from "../middleware/auth";

const user = authentication();

if (user) {
    return new Response(null, {
        status: 302,
        headers: {
            Location: "/",
        }
    });
}

---
<Layout title="Authenticcation test">
<h1>Welcome logged in {user}</h1>
</Layout>